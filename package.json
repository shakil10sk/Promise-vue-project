        "vue-template-compiler": "^2.6.14"
    },
    "dependencies": {
        "apexcharts": "^3.35.0",
        "chart.js": "^3.7.1",
        "vue": "^2.6.14",
        "vue-apexcharts": "^1.6.2",
        "watch": "^1.0.2",
        "vue-router": "^3.2.0",
        "vue2-datepicker": "^3.10.4",
        "simple-vue-validator": "^0.16.0",
